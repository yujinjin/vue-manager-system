        "postcss-html": "^1.5.0",
        "postcss-less": "^6.0.0",
        "prettier": "~3.0.3",
        "ts-node": "^10.9.1",
        "typedoc": "^0.25.3",
        "typescript": "~5.2.2",
        "eslint": "^8.52.0",
        "eslint-define-config": "~1.24.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-vue": "^9.18.1",
