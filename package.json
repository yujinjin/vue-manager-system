{
    "name": "vue-manager-system",
    "version": "0.1.0",
    "description": "基于vue的element搭建的后台管理系统框架",
    "main": "src/main.ts",
    "module": "src/main.ts",
    "private": false,
    "keywords": [
        "h5"
    ],
    "homepage": "https://yujinjin.github.io/",
    "bugs": {
        "email": "yujinjin9@126.com",
        "url": "https://github.com/yujinjin/vue-manager-system/issues"
    },
    "author": {
        "name": "jack yu",
        "email": "yujinjin9@126.com",
        "url": "https://yujinjin.github.io/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yujinjin/vue-manager-system.git"
    },
    "scripts": {
        "R_DEV": "vue-cli-service serve --mode dev --copy",
        "R_UAT": "vue-cli-service serve --mode uat --copy",
        "R_PRD": "vue-cli-service serve --mode prd --copy",
        "REPORT": "vue-cli-service build --mode prd --report",
        "B_DEV": "vue-cli-service build --mode dev",
        "B_UAT": "vue-cli-service build --mode uat",
        "B_PRE": "vue-cli-service build --mode pre",
        "B_PRD": "vue-cli-service build --mode prd",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^3.21.1",
        "axios": "^0.24.0",
        "dayjs": "^1.10.7",
        "vue": "^3.2.26",
        "vuex": "^4.0.2",
