        "h5"
    ],
    "homepage": "https://yujinjin.github.io/",
    "bugs": {
        "email": "yujinjin9@126.com",
        "url": "https://github.com/yujinjin/vue-manager-system/issues"
    },
    "author": {
        "name": "jack yu",
        "email": "yujinjin9@126.com",
        "url": "https://yujinjin.github.io/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yujinjin/vue-manager-system.git"
    },
    "scripts": {
        "mserve": "cross-env MOCK_DATA=1 vue-cli-service serve --mode dev --copy",
        "dev": "vue-cli-service serve --mode dev --copy",
        "uat": "vue-cli-service serve --mode uat --copy",
        "prd": "vue-cli-service serve --mode prd --copy",
        "report": "vue-cli-service build --mode prd --report",
        "B_DEV": "vue-cli-service build --mode dev",
        "B_UAT": "vue-cli-service build --mode uat",
        "B_PRE": "vue-cli-service build --mode pre",
        "B_PRD": "vue-cli-service build --mode prd",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^3.33.1",
        "vue": "^3.3.7",
        "pinia": "^2.1.7",
        "vue-router": "^4.2.5",
        "axios": "^1.6.0",
        "dayjs": "^1.11.10",
        "element-plus": "^2.4.4",
        "@element-plus/icons-vue": "^2.1.0",
        "vue-types": "^5.1.1",
        "echarts": "^5.4.3",
        "qrcode": "^1.5.3",
        "cropperjs": "^1.6.1",
        "quill": "^1.3.7",
        "@yujinjin/utils": "^0.0.2",
        "@better-scroll/core": "^2.5.1",
        "@better-scroll/mouse-wheel": "^2.5.1"
    },
    "devDependencies": {
        "alphabetjs": "^0.1.4",
