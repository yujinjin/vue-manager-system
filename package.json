        "vue-types": "^4.1.1",
        "echarts": "^5.3.0",
        "element-plus": "^2.0.5"
    },
    "devDependencies": {
        "mockjs": "^1.1.0",
        "alphabetjs": "^0.1.4",
        "prettier": "^2.5.1",
        "less": "^4.1.2",
        "less-loader": "^7.3.0",
        "node-sass": "^4.14.1",
        "sass-loader": "^9.0.3",
        "postcss": "^8.4.5",
