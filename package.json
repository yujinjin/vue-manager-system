		"url": "https://github.com/yujinjin/vue-manager-system.git"
	},
	"scripts": {
		"R_DEV": "vue-cli-service serve --mode dev --copy",
		"R_UAT": "vue-cli-service serve --mode uat --copy",
		"R_PRD": "vue-cli-service serve --mode prd --copy",
		"REPORT": "vue-cli-service build --mode prd --report",
		"B_DEV": "vue-cli-service build --mode dev",
		"B_UAT": "vue-cli-service build --mode uat",
		"B_PRE": "vue-cli-service build --mode pre",
		"B_PRD": "vue-cli-service build --mode prd",
		"test:unit": "vue-cli-service test:unit",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^3.21.0",
		"vue": "^2.6.14",
		"vue-router": "^3.5.1",
		"vuex": "^3.6.2",
		"axios": "^0.25.0",
		"element-ui": "^2.15.6",
		"jquery": "^1.12.4",
		"toastr": "^2.1.4",
		"cropperjs": "^1.5.12",
		"echarts": "^5.3.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.13",
		"@vue/cli-plugin-eslint": "~4.5.13",
		"@vue/cli-plugin-router": "~4.5.13",
		"@vue/cli-plugin-unit-jest": "~4.5.13",
