{
    "name": "vue-manager-system",
    "version": "0.1.0",
    "description": "基于vue的element搭建的后台管理系统框架",
    "main": "src/main.ts",
    "module": "src/main.ts",
    "private": false,
    "keywords": [
        "h5"
    ],
    "homepage": "https://yujinjin.github.io/",
    "bugs": {
        "email": "yujinjin9@126.com",
        "url": "https://github.com/yujinjin/vue-manager-system/issues"
    },
    "author": {
        "name": "jack yu",
        "email": "yujinjin9@126.com",
        "url": "https://yujinjin.github.io/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yujinjin/vue-manager-system.git"
    },
    "scripts": {
        "dev": "vue-cli-service serve --mode dev --copy",
        "uat": "vue-cli-service serve --mode uat --copy",
        "prd": "vue-cli-service serve --mode prd --copy",
        "report": "vue-cli-service build --mode prd --report",
        "B_DEV": "vue-cli-service build --mode dev",
        "B_UAT": "vue-cli-service build --mode uat",
        "B_PRE": "vue-cli-service build --mode pre",
        "B_PRD": "vue-cli-service build --mode prd",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^3.21.1",
        "vue": "^3.2.41",
        "pinia": "^2.0.23",
        "vue-router": "^4.1.5",
        "axios": "^1.1.3",
        "dayjs": "^1.11.6",
        "element-plus": "^2.2.26",
        "@element-plus/icons-vue": "^2.0.10",
        "vue-types": "^5.0.1",
        "echarts": "^5.4.0",
        "mockjs": "^1.1.0"
    },
    "devDependencies": {
        "alphabetjs": "^0.1.4",
        "less": "^4.1.3",
        "less-loader": "^8.0.0",
        "sass": "^1.50.0",
        "sass-loader": "^12.6.0",
        "postcss": "^8.4.18",
        "postcss-html": "^1.5.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.7.1",
        "ts-node": "^10.4.0",
        "typedoc": "^0.22.10",
        "typescript": "~4.1.5",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-define-config": "^1.2.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "@types/mockjs": "^1.0.4",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "@vue/cli-plugin-babel": "~5.0.8",
        "@vue/cli-plugin-eslint": "~5.0.8",
        "@vue/cli-service": "~5.0.8",
        "@vue/cli-plugin-router": "~5.0.8",
        "@vue/cli-plugin-vuex": "~5.0.8",
        "@vue/cli-plugin-typescript": "~5.0.8",
        "@vue/cli-plugin-unit-mocha": "~5.0.8",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "svg-sprite-loader": "^6.0.9"
    },
    "prettier": {
        "tabWidth": 4,
        "useTabs": false,
        "jsxBracketSameLine": false,
        "printWidth": 200,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "semi": true,
        "singleQuote": false,
        "htmlWhitespaceSensitivity": "ignore"
    },
    "engines": {
        "node": ">=14"
    },
    "lint-staged": {
        "src/**/*.{jsx,txs,ts,js,json,vue,md}": [
            "vue-cli-service lint",
            "git add"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
